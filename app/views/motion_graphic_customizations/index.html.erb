<h1>Listing Motion Graphic Presets</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Motion graphic</th>
      <th>Custom field data</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @motion_graphic_customizations.each do |motion_graphic_customization| %>
      <tr>
        <td><%= motion_graphic_customization.user %></td>
        <td><%= motion_graphic_customization.motion_graphic_id %></td>
        <td><%= motion_graphic_customization.custom_field_data %></td>
        <td><%= link_to 'Show', motion_graphic_customization %></td>
        <td><%= link_to 'Edit', edit_motion_graphic_customization_path(motion_graphic_customization) %></td>
        <td><%= link_to 'Destroy', motion_graphic_customization, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Motion graphic customization', new_motion_graphic_customization_path %>
